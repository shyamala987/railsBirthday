<h1>Log in</h1>

<%= form_with url: sessions_path do |form| %>
    <div>
      <%= form.label :email %><br>
      <%= form.text_field :email %>
    </div>
  
    <div>
      <%= form.label :password %><br>
      <%= form.password_field :password %>
    </div>

    <div>
      <%= form.submit %>
    </div>
<% end %>
